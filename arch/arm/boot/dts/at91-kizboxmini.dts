#include "at91-kizboxmini_common.dtsi"

/ {
	model = "Overkiz Kizbox Mini";
	compatible = "overkiz,kizboxmini-1", "atmel,at91sam9g25", "atmel,at91sam9x5", "atmel,at91sam9";
